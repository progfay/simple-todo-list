  "bugs": {
    "url": "https://github.com/progfay/simple-todo-list/issues"
  },
  "homepage": "https://github.com/progfay/simple-todo-list#readme",
  "devDependencies": {
    "now": "^15.2.0",
    "standard": "^12.0.1",
    "workbox-cli": "^4.3.1"
  }
}
